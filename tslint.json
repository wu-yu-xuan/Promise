{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "rules": {
    "quotemark": false,
    "trailing-comma": false,
    "ordered-imports": false,
    "interface-name": false,
    "radix": false,
    "member-access": false,
    "no-bitwise": false,
    "arrow-parens": false,
    "no-conditional-assignment": false,
    "no-unnecessary-initializer": false,
    "max-line-length": [true, 140]
  }
}
